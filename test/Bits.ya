package Bits where

def Bits : *
  = ∀.self
  ( P : ∀ Bits -> *)
  ( be: P (λ P be b0 b1 => be) )
  ( b0: ∀ (pred: Bits) -> P (λ P be b0 b1 => b0 pred))
  ( b1: ∀ (pred: Bits) -> P (λ P be b0 b1 => b1 pred))
  -> P self

def be : Bits = λ P be b0 b1 => be
def b0 (pred : Bits) : Bits = λ P be b0 b1 => b0 pred
def b1 (pred : Bits) : Bits = λ P be b0 b1 => b1 pred
